<div class="container">
  <div class="col-md-8 col-md-offset-2">
    <% @messages.each do |message| %>
        <div class="message">
          <h2 class="message-title"><%= message.title %></h2>
          <%= link_to "Join", message, class: 'btn btn-info' %>
        </div>

    <% end %>
  </div>
</div>